    {
      "name": "Mr. VnS",
      "email": "mrvns@live.com"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.1|^7.0",
    "nesbot/carbon": "2.*",
    "league/flysystem": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "~4.0||~5.0",
    "scrutinizer/ocular": "~1.1",
